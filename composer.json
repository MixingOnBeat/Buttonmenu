{
	"name": "dmzx/buttonmenu",
	"type": "phpbb-extension",
	"description": "This extension adds a menu under the header (similar to phpbb.com menu) fully configurable via ACP.",
	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.0",
	"time": "2015-08-06",
	"keywords": ["phpbb", "extension", "Button menu"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"extra": {
		"display-name": "Button menu",
		"soft-require": {
			"phpbb/phpbb": "3.1.*"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "buttonmenu_version.json"
		}
	}
